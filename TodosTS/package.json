{
  "name": "TodosTS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run build && jest --coverage",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc -- ",
    "lint": "tslint src/**/*.ts",
    "watch": "npm run build -- -w",
    "start:android": "npm run build && concurrently -r \"react-native run-android \" \"npm run watch\""
  },
  "dependencies": {
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-router-flux": "^3.39.2",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-persist": "^4.8.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.11"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/react": "^15.0.24",
    "@types/react-native": "^0.44.0",
    "@types/react-redux": "^4.4.40",
    "@types/redux": "^3.6.0",
    "babel-jest": "20.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "1.9.2",
    "concurrently": "^3.4.0",
    "enzyme": "^2.8.2",
    "jest": "20.0.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-native-cli": "^2.0.1",
    "react-native-mock-render": "0.0.6",
    "react-test-renderer": "^16.0.0-alpha.6",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "tslint-fix": "^0.1.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "artifacts/.+\\.(test|spec).(ts|tsx|js)$",
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "artifacts/**/*.js",
      "!artifacts/**/*.spec.js",
      "!artifacts/**/*.index.js"
    ],
    "testEnvironment": "jsdom"
  }
}
